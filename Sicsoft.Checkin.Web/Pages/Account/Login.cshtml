@page
@model Sicsoft.Checkin.Web.LoginModel
@{
    Layout = null;
}

<!DOCTYPE html>

<html lang="es">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Inicio de sesión</title>
    <link rel="stylesheet" href="/css/login/vendor/bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" href="/css/login/fonts/font-awesome-4.7.0/css/font-awesome.min.css" />
    <link rel="stylesheet" href="/css/login/fonts/iconic/css/material-design-iconic-font.min.css" />
    <link rel="stylesheet" href="/css/login/vendor/animate/animate.css" />
    <link rel="stylesheet" href="/css/login/vendor/css-hamburgers/hamburgers.min.css" />
    <link rel="stylesheet" href="/css/login/vendor/animsition/css/animsition.min.css" />
    <link rel="stylesheet" href="/css/login/vendor/select2/select2.min.css" />
    <link rel="stylesheet" href="/css/login/vendor/daterangepicker/daterangepicker.css" />
    <link rel="stylesheet" href="/css/login/css/util.css" />
    <link rel="stylesheet" href="/css/login/css/main.css" />
  

    <link rel="icon" href="/jpg2.jpg" type="image/png" />
</head>
 
<body>

    <div class="limiter">
        <div class="container-login100">
            <div class="wrap-login100">
                <form class="login100-form validate-form" method="post" id="formTipos" role="form" novalidate  enctype="multipart/form-data">
                    <span class="login100-form-title p-b-26">
                        Bienvenido
                    </span>
                    <span class="login100-form-title p-b-48">
                        <i class="zmdi zmdi-home"></i>
                    </span>
                    <div class="wrap-input100 validate-input" data-validate="Valid email is: a@b.c">
                        <input class="input100" asp-for="Input.Email" type="email" >
                        <span class="focus-input100" data-placeholder="Email"></span>
                    </div>
                    <div class="wrap-input100 validate-input" data-validate="Enter password">
                        <span class="btn-show-pass">
                            <i class="zmdi zmdi-eye"></i>
                        </span>
                        <input class="input100" asp-for="Input.Password" type="password" >
                        <span class="focus-input100" data-placeholder="Password"></span>
                    </div>
                        <div asp-validation-summary="All" style="text-align: center;" class="text-danger"></div>
                    <div class="container-login100-form-btn">
                        <div class="wrap-login100-form-btn">
                            <div class="login100-form-bgbtn"></div>
                            <button class="login100-form-btn" type="submit">
                                Login
                            </button>
                        </div>
                    </div>
                   
                </form>
            </div>
        </div>
    </div>
    <div id="dropDownSelect1"></div>

    <!-- Mainly scripts -->
    <script src="~/css/login/vendor/jquery/jquery-3.2.1.min.js"></script>
    <script src="~/css/login/vendor/animsition/js/animsition.min.js"></script>
    <script src="~/css/login/vendor/bootstrap/js/popper.js"></script>
    <script src="~/css/login/vendor/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/css/login/vendor/select2/select2.min.js"></script>
    <script src="~/css/login/vendor/daterangepicker/moment.min.js"></script>
    <script src="~/css/login/vendor/daterangepicker/daterangepicker.js"></script>
    <script src="~/css/login/vendor/countdowntime/countdowntime.js"></script>
    <script src="~/css/login/js/main.js"></script>

 
    <script src="~/js/site.js" asp-append-version="true"></script>




</body>
</html>
